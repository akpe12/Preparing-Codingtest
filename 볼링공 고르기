n, m = map(int, input().split())
weight = list(map(int, input().split()))

# 조합의 개수 but 같은 무게는 불가
# 같은 무게일 때만 넘기라는 조건 사용

arr = []

for i in range(len(weight)):
    curr = weight[i]
    for j in range(i + 1, len(weight)):
        if curr == weight[j]:
            continue
        else:
            arr.append([curr, weight[j]])
print(len(arr))
