import sys

nodes, edges, start = map(int, sys.stdin.readline().split())
dest_graph = [[] for _ in range(nodes + 2)] #graph 항의 개수를 다시 계산해야될 것 같다.
visited = [False] * (nodes + 1)

for i in range(edges):
    src, dest = map(int, sys.stdin.readline().split())
    dest_graph[src].append(dest)
print(dest_graph)

def dfs(graph, num, visited):
    visited[num] = True
    print(num, end = " ")
    
    for i in graph[num]:
        if visited[i] != True:
            dfs(graph, i, visited)

dfs(dest_graph, start, visited)
