#1
import sys

n, k = map(int, sys.stdin.readline().split())
#동전의 종류를 담을 리스트를 생성한다.
kind_of_coins = []
for _ in range(n):
    kinds = int(sys.stdin.readline().rstrip())
    kind_of_coins.append(kinds)
#값어치가 큰 동전의 종류부터 사용하기 위해 내림차순으로 정렬한다.
kind_of_coins.sort(reverse = True)
count = 0

for coin in kind_of_coins:
    #만드려는 돈보다 kind_of_coins 리스트에서 만난 동전의 가치와 같거나 그것보다 낮다면:
    #만드려는 돈보다 값어치가 더 큰 동전으로는 원하는 값을 만들 수가 없기 때문이다.
    if k >= coin:
        #count에 k를 coin으로 나눈 몫만큼 더해준다.
        count += k // coin
        #coin을 이용해서 만든 가치만큼 k에서 빼준다.
        k %= coin
        
print(count)


#2
import sys

n, k = map(int, sys.stdin.readline().split())
#시간 비교를 위해 item이 0인 그리고 길이가 n인 리스트를 생성했다.
kind_of_coins = [0] * n
for i in range(n):
    kinds = int(sys.stdin.readline().rstrip())
    kind_of_coins[i] = kinds
kind_of_coins.sort(reverse = True)
count = 0

for coin in kind_of_coins:
    if k >= coin:
        count += k // coin
        k %= coin
        
print(count)
