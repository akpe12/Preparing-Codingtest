import sys

sugar = int(sys.stdin.readline().rstrip())
count = 0

while True:
    #sugar가 5로 완전히 나누어진다면:
    if sugar % 5 == 0:
        count += sugar // 5
        sugar %= 5
        print(count)
        break
    #sugar가 어떤 수에 나눠지는가에 상관없이
    #sugar가 5로 완전히 나눠질 수 있도록
    #이 단계에서 sugar에서 3kg을 차감시킨다.
    sugar -= 3
    count += 1
    #3을 빼다가 sugar가 0이하의 수가 되었다면:
    #5kg과 3kg으로 나눌 수 없는 수라는 걸 나타내므로
    #-1 처리한다.
    if sugar < 0:
        print(-1)
        break
