import sys
import math
input = sys.stdin.readline()

n = int(input.rstrip())
prime_num = [True for i in range(n + 1)]

def how_prime_number(n):
    ans = 0
    for i in range(2, n + 1):
        if prime_num[i] == True:
            j = 2
            while i * j <= n:
                prime_num[i * j] = False
                j += 1
                
    for j in range(2, n):
        if prime_num[j]:
            ans += 1
    return ans

print(how_prime_number(n))
